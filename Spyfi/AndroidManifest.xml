<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.nraboy.spyfi"
      android:versionCode="7"
      android:versionName="1.3.2">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission> 
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:name="mainactivity"
                  android:label="@string/app_name"
                  android:icon="@drawable/icon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="stream" android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"></activity>
        <activity android:name="addcamera"></activity>
        <activity android:name="updatecamera"></activity>
        <activity android:name="about" android:screenOrientation="portrait"></activity>
        
    </application>
</manifest> 
